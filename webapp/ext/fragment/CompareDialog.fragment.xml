<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog id="compareDialogId"
        title="{i18n>COMPARE}"
       core:require="{ controller: 'irp/matchinboundstatement/ext/controller/MatchedComparision' }" horizontalScrolling="true" contentWidth="70em">   
        <Table id="idTable"  growingScrollToLoad="true">    
        </Table>
        <endButton>
         <Button
          type="Emphasized"
            text="{i18n>OK}"
            press="controller.closeDialog"/>
      </endButton>
    </Dialog>
</core:FragmentDefinition>